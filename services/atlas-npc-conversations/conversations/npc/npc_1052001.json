    },
    {
      "id": "secondJobComplete4Assassin",
      "type": "dialogue",
      "dialogue": {
        "dialogueType": "sendNextPrev",
        "text": "Assassin need to be strong. But remember that you can't abuse that power and use it on a weakling. Please use your enormous power the right way, because... for you to use that the right way, that is much harden than just getting stronger. Please find me after you have advanced much further. I'll be waiting for you.",
        "choices": [
          {"text": "Previous", "nextState": "secondJobComplete3Assassin"},
          {"text": "Next", "nextState": null},
          {"text": "Exit", "nextState": null}
        ]
      }
